		"class-validator": "^0.14.2",
		"class-validator-jsonschema": "^5.0.2",
		"cls-hooked": "^4.2.2",
		"consul": "^2.0.1",
		"cron": "^4.3.1",
		"dotenv": "^16.5.0",
		"envalid": "^8.0.0",
		"guid-typescript": "^1.0.9",
		"http-status-codes": "^2.3.0",
		"ioredis": "^5.6.1",
		"kafkajs": "^2.2.4",
		"linq": "^3.2.4",
		"mediatr-ts": "^2.0.2",
		"neverthrow": "^8.2.0",
